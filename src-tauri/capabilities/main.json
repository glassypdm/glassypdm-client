{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:path:default",
    "core:event:default",
    "core:window:default",
    "core:app:default",
    "core:resources:default",
    "core:menu:default",
    "core:tray:default",
    "shell:allow-open",
    "dialog:allow-open",
    "fs:allow-exists",
    "fs:allow-mkdir",
    "notification:allow-is-permission-granted",
    "notification:allow-request-permission",
    "notification:default",
    "log:default"
  ]
}